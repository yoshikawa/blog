{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "./public",
      "url": [
        "/"
      ]
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off",
        "canonical": "off"
      }
    }
  }
}
